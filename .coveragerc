[run]
source = .
omit =
    */tests/*
    */test_*.py
    setup.py
    __main__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abstract.property
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
